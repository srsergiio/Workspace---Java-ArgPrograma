package GUIA1;

public class Piesa {

	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	public String getString() {
		return posicion;
	}
	public void setposicion(String string) {
		posicion = string;
	}

	String nombre;
	String posicion;
	
	public  Piesa(String n,String p) {
		 nombre = n;
		 posicion = p;
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Piesa fichaUNO = new Piesa("alfil","a1");
	}

}
